{
  "name": "bases",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:href": "ng build --base-href ./",
    "build:github": "npm run build:href && npm run reorder:dist && npm run delete:docs && npm run copy:dist && npm run delete:dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "reorder:dist": "npm run copy:browsercontent && npm run delete:browser",
    "copy:browsercontent": "copyfiles dist/bases/browser/* dist/bases -f",
    "delete:browser": "del-cli dist/02-bases/browser",
    "copy:dist": "copyfiles dist/bases/* ./docs -f",
    "delete:docs": "del-cli docs",
    "delete:dist": "del-cli dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^10.0.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "@types/uuid": "^10.0.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.2"
  }
}
